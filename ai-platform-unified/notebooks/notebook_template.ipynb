        "model = tf.keras.Sequential(\n",
        "    [\n",
        "        tf.keras.layers.Dense(10, activation=\"relu\", input_shape=(None, 5)),\n",
        "        tf.keras.layers.Dense(3),\n",
        "    ]\n",
        ")"
      ]
